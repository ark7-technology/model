{
  "name": "@ark7/model",
  "version": "2.0.43",
  "description": "Ark7 model used for both backend and frontend",
  "types": "index.d.ts",
  "main": "index.js",
  "scripts": {
    "build": "scripts/build",
    "clean": "rm -rf dist",
    "compile:src": "tsc --build src/tsconfig.json",
    "compile:src:pure": "tsc --build src/tsconfig.pure.json",
    "compile:test": "npm run compile:src && tsc --build test/tsconfig.json",
    "release": "scripts/release",
    "test": "npm run compile:src && NODE_ENV=test node_modules/.bin/mocha --trace-warnings --require ts-node/register \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ark7-technology/model.git"
  },
  "author": "yz@ark7.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ark7-technology/model/issues"
  },
  "homepage": "https://github.com/ark7-technology/model#readme",
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/mocha": "^8.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.11.9",
    "@types/should": "^13.0.0",
    "change-case": "^4.1.2",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^9.2.2",
    "moment": "^2.29.4",
    "should": "^13.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/underscore": "^1.10.24",
    "debug": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "underscore": "^1.13.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "renke"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "renke"
    }
  }
}
